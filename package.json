{
  "name": "relajet-graphql-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "NODE_ENV=development DEBUG=dev:* npm-run-all run-webpack:dev --parallel run-webpack:dev:watch run-nodemon",
    "start:prod": "NODE_ENV=production npm-run-all -s run-webpack:prod run-node",
    "run-webpack:dev": "webpack --config ./build/webpack.config.dev.js ./src/server.js",
    "run-webpack:dev:watch": "webpack --config ./build/webpack.config.dev.js --watch ./src/server.js",
    "run-webpack:prod": "webpack --config ./build/webpack.config.prod.js ./src/server.js",
    "run-nodemon": "nodemon dist/server.build.js",
    "run-node": "node dist/server.build.js",
    "make-migration": "knex migrate:make",
    "run-migration:dev": "knex migrate:latest --env development",
    "make-seed": "knex seed:make",
    "run-seed:dev": "knex seed:run --env development",
    "run-rollback": "knex migrate:rollback",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "vergil",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "apollo-server-koa": "^1.3.6",
    "boom": "^7.2.0",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "graphql": "^0.13.2",
    "graphql-playground-middleware-koa": "^1.5.1",
    "graphql-tools": "^3.0.0",
    "knex": "^0.14.6",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^4.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "pg": "^7.4.3",
    "ramda": "^0.25.0",
    "supports-color": "^5.4.0"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "graphql-import-loader": "^0.2.1",
    "nodemon": "^1.17.4",
